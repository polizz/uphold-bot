{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": ["error", { "allow": ["warn", "error"] } ],
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "argsIgnorePattern": "_" }]
    }
}
